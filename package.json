{
  "name": "beam-bvm-interface",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "rust-bindgen": "npm-run-all --parallel rust-bindgen:**",
    "rust-bindgen:current-branch": "cross-env bindgen \"./beam/bvm/Shaders/common.h\" -o \"./build/current-branch/src/lib.rs\" --no-layout-tests --enable-cxx-namespaces --distrust-clang-mangling --allowlist-type Env.* --allowlist-function Env.* --allowlist-type Env.* --allowlist-var Env.* -- -x c++ -I \"%BOOST_ROOT%\" -I \"./beam\"",
    "rust-bindgen:select-branch": "powershell \".\\build.ps1\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NewDark90/beam-bvm-interface.git"
  },
  "author": "",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/NewDark90/beam-bvm-interface/issues"
  },
  "languages": [
    {
      "id": "cpp",
      "extensions": [
        ".hpp"
      ]
    }
  ],
  "homepage": "https://github.com/NewDark90/beam-bvm-interface#readme",
  "dependencies": {
    "cpy": "^8.1.2",
    "fs-extra": "^10.0.0",
    "wasm2c": "^1.0.0",
    "wasm2js": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.4.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5"
  }
}
